<table class="table table-bordered">
  <thead>
    <tr>
      <td>Date</td>
      <td>Title</td>
      <td>Location</td>
      <td>Host</td>
    </tr>
  </thead>
  <% object.each do |event| %>
    <tr>
      <td><%= event.date %></td>
      <td><%= link_to event.name, event_path(event)  %></td>
      <td><%= event.location  %></td>
      <td><%= event.creator.name %></td>
    </tr>
  <% end %>   
</table>
